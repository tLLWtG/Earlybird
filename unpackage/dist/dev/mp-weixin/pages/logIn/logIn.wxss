/* 水平间距 */
.cont {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-content: center;
  font-size: 50rpx;
  line-height: 80rpx;
}
.imageContainer {
  display: flex;
  justify-content: center;
  margin-top: 100rpx;
  margin-bottom: 0rpx;
  margin: 100rpx auto 50rpx auto;
  width: 750rpx;
  height: 300rpx;
}
.imageContainer image {
  height: 200rpx;
  width: 200rpx;
}
.inputContianer {
  display: flex;
  flex-direction: column;
  width: 650rpx;
  margin: 0 auto;
}
.inputContianer .inputs {
  width: 650rpx;
  height: 50rpx;
  margin-bottom: 20rpx;
  border: 2rpx solid #D3D3D3;
  border-radius: 10rpx;
}
.logInButton {
  width: 650rpx;
  height: 100rpx;
  margin: 0 auto;
  background-color: #F5F5DC;
  font-size: 50rpx;
  text-align: center;
  line-height: 100rpx;
}

